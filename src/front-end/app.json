{
  "expo": {
    "name": "front-end",
    "slug": "front-end",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoHomeLight.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/LogoHomeLight.png",
      "resizeMode": "contain",
      "backgroundColor": "#EDEDED"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoHomeLight.png",
        "backgroundColor": "#EDEDED"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.samuelmaia.dev.frontend"
    },
    "web": {
      "favicon": "./assets/images/LogoHomeLight.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "A aplicação precisa acessar suas fotos para fazer upload de imagens.",
          "cameraPermission": "A aplicação precisa acessar sua câmera para tirar fotos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "046448d8-c72a-4286-a712-99b3ceee0635"
      }
    }
  }
}
